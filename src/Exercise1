        }
    }
}

// zeros at end 10101 => 11100

import java.util.Scanner; 
public class Main{
public static void main(String[] args) {
            Scanner sc = new Scanner(System.in);
            long n = sc.nextInt();
			int a=0;
			int b=0;
			long sum=0;
			while(n>0) {
			    if(n%10==1){
				 a++;
				 sum=sum*10+1;
				 System.out.println(sum);
			    }
			    else b++;
			      n/=10;
			    
			}
			sum =sum*(long) Math.pow(10,b);
		    System.out.println(sum);
	}
}
/******************************************************************************

Input :
6
12 23 35 56 78 90
112

Output :
3
45 67 9 75 12 69 -> Started Outer for loop 1st 

45>67 ->F skip
67>9 ->T Swap

45 9 67 75 12 69

67>75 -> F skip
75>12 ->T Swap

45 9 67 12 75 69

75>69 -> T Swap

45 9 67 12 69 75 -> end Outer for loop 1st
45 9 67 12 69 75 ->Started Outer for loop 2nd iteration 
45 > 9 ->T Swap

9 45 67 12 69 75

45>67 - F skip
67>12 -> T Swap

9 45 12 67 69 75

67>69 - F skip 
69>75 - F skip

9 45 12 67 69 75 ->3rd outer loop iteration  
9>45 - F skip 
45 > 12 - T Swap

9 12 45 67 69 75 
*******************************************************************************/
import java.util.*;
public class Main
{   //Linear Search
    public static int linears(int[] arr , int key){
        for(int i = 0 ; i < arr.length ; i++){
            if(arr[i]==key) 
                return i;
        }return -1;
    }
    //binary search
    public static int binarys(int[] arr,int key){
        int left = 0;
        int right = arr.length-1;
        while(left<=right){
            int mid = (left+right)/2;
            if(arr[mid]==key){
                return mid;
            }
            if(key>arr[mid]){
                left = mid+1;
            }
            if(key<arr[mid]){
                right = mid-1;
            }
        }
        return -1;
    }
    //Bubble Sort
    public static void bubblesort(int[] arr){
        for(int i = 0 ; i<arr.length-1; i++){
            for(int j = 0 ; j < arr.length-1;j++){
                if(arr[j]>arr[j+1]){
                    int t = arr[j];
                    arr[j] = arr[j+1];
                    arr[j+1] = t;
                }
            }
        }
    }
    public static void display(int[] arr){
    
        for(int i = 0 ; i < arr.length ; i++){
            System.out.print(arr[i]+" ");
        }
        System.out.println();
    }
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int[] arr = new int[n];
		for(int i = 0 ; i < n ; i++){
		    arr[i] = sc.nextInt();
		}
		int key = sc.nextInt();
		display(arr);
		bubblesort(arr);
		display(arr);
		System.out.println("Target value present in : "+binarys(arr,key));
	}
}

// zig zag sum of array
import java.util.*;
public class Main
{
    public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int x=sc.nextInt();
		int y=sc.nextInt();
		int sum=0;
	    int a[][]=new int[x][y];
		for(int i=0;i<x;i++) {
			for(int j=0;j<y;j++) {
				a[i][j]=sc.nextInt();
				if((i==0) || (i+j==x-1) || (i==x-1)){
                        sum += a[i][j];
                }
			}
		}
		System.out.println("The Sum of Zig-Zag: "+sum);
	
	}
	}
// product of the array 
import java.util.*;
public class Main
{
    public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int[] a= {10,3,5,6,2};
    int product = 1;
    for(int z:a) {
        product *= z;
}
    for(int i = 0; i < a.length; i++) {
        a[i] = product/a[i];
}
    for(int i: a) {
        System.out.print(i+" ");
}
}
}
//Find missing number
public class Main {

    public static int findMissingNumber(int[] arr) {
        int n = arr.length + 1;
        int e = n * (n + 1) / 2;
        int Sum = 0;
        for (int num : arr) {
            Sum += num;
        }
        return (int) (e - Sum);
    }

    public static void main(String[] args) {
        int[] arr1 = {8, 2, 4, 5, 3, 7, 1};
        System.out.println("Output: " + findMissingNumber(arr1));
        
        int[] arr2 = {1, 2, 3, 5};
        System.out.println("Output: " + findMissingNumber(arr2)); 
    }
}
//leedcode 609
class Solution {
    public boolean detectCapitalUse(String word) {
        int n = word.length();
        char c = word.charAt(0);
        if (c >= 'A' && c <= 'Z') {
            if (n > 1 && word.charAt(1) >= 'A' && word.charAt(1) <= 'Z') {
                for (int i = 2; i < n; i++) {
                    if (word.charAt(i) < 'A' || word.charAt(i) > 'Z') {
                        return false;
                    }
                }
                return true;
            }
            else {
                for (int i = 1; i < n; i++) {
                    if (word.charAt(i) < 'a' || word.charAt(i) > 'z') {
                        return false;
                    }
                }
                return true;
            }
        }
        else {
            for (int i = 1; i < n; i++) {
                if (word.charAt(i) < 'a' || word.charAt(i) > 'z') {
                    return false;
                }
            }
            return true;
        }
    }
}

// Palindrome using methods
import java.util.*;
public class Main{
    public static void main(String[]args){
        String s="HELLO";
        int l=0;
        int r=s.length()-1;
        while (l < r) {
            if (s.charAt(l) != s.charAt(r)) {
                System.out.println("not palindrome");
                return;
            }
            l++;
            r--;
        }
                System.out.println("Palindrome");
          
        }
    }

//Anagram
import java.util.*;
public class Main{
    
        public static void Anagram(String s1 ,String s2){
                if(s1.length()!=s2.length()) {
                  System.out.println("Not a Anagram");
                return;
            }
             int[] freq = new int[26];
             for(int i = 0 ; i < s1.length(); i++){
             int c1=s1.charAt(i)-97;
             int c2=s2.charAt(i)-97;
             freq[c1]++;
             freq[c2]--;
             }
         for(int i = 0 ; i < 26; i++){
           if(freq[i]!=0) {
            System.out.println("Not a anagram");
           return;
          }
       
    }
    System.out.println("Its a anagram");
}
    public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    String s1 = sc.next().toLowerCase();
    String s2 = sc.next().toLowerCase();
    Anagram (s1,s2);

        
    }
}

