// Conversion:dollar to cents
import java.util.Scanner;
class Main{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter The value:");
        int n=sc.nextInt();
        System.out.println("Enter The dollar:");
        int dollar=sc.nextInt();
        System.out.println("Enter The Cents:");
        int cents =sc.nextInt(); 
        int dollar1=sc.nextInt();
        int cents2=sc.nextInt(); 
        int Sum=dollar+dollar1;
        int sumofCents=cents+cents2;
        int c=sumofCents/100;
        int E=Sum+c;
        int D=sumofCents%100;
        System.out.println(E);
        System.out.println(D);
        
    }
}

// Adding Hours and Time and Second 
import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.println("Enter The hour:");
        int hr = sc.nextInt();
        System.out.println("Enter The Minutes:");
        int min = sc.nextInt();
        System.out.println("Enter The second:");
        int sec = sc.nextInt();

        System.out.println("Enter The hr2:");
        int hr2 = sc.nextInt();
        System.out.println("Enter The min2:");
        int min2 = sc.nextInt();
        System.out.println("Enter The second2:");
        int sec2 = sc.nextInt();

        // Add seconds
        int a = sec + sec2;
        int c= a / 60;
        int Sec = a % 60;

        // Add minutes
        int Min = min + min2 + c;
        int Hr = Min / 60;
        int n = Min % 60;

        // Add hours
        int f = hr + hr2 + Hr;

        System.out.println("Hours: " + f);
        System.out.println("Minutes: " + n);
        System.out.println("Seconds: " + Sec);
    }
}

//Tik toe
import java.util.Scanner;
public class Main {
	
		public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int Row=(a-1)/3;
		int column=(a-1)%3;
		System.out.println(Row);
	    System.out.println(column);
	}
}

/*Electricity Bill
In the city of Rajkot, Gujarat the Golden company is taking care of the expenses for the Court's discussion room. Due to continuous discussions regarding the upcoming 'Ring Road' project at Rajkot, the electricity bill was high and they need to pay it with all the money they have.
The electricity board has decided to charge money based on the units consumed by a particular home. If the units consumed are less than or equal to 200, the cost for one unit is 0.5paise. If the unit is less than or equal to 400, the cost for one unit is 0.65paise plus Rs.100 extra charge. If the unit is less than or equal to 600, the cost for one unit is 0.80paise plus Rs.200 extra charge. If the unit is greater than 600 the cost for one unit is Rs.1.25 plus Rs.425 extra charge. You need to calculate the electricity bill based on the units consumed
*/
import java.util.Scanner;
public class Main{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        int unit=sc.nextInt();
        if(unit<=200)
        {
            double price=0.5*unit;
            System.out.println(price);
        }
        else if(unit<=400){
             double pr=0.65*unit+100.0;
            System.out.println(pr);
        }
        else if(unit<=600){
             double p=0.80*unit+200.0;
            System.out.println(p);
        }
        else if (unit > 600){
             double pri=1.25*unit+425.0;
            System.out.println(pri);
         }
    }
}

//Polymorphism:
//i)overloading
import java.util.*;
class file{
    void display(String s){
        System.out.println("This is:"+s);
    }
     void display(int n){
        System.out.println("Polymorphism"+n+"types");
    }
     void display(String s,int a){
        System.out.println("This is "+a+" method:" +s);
    }
}
class Main{
    public static void main(String[]args){
        file f=new file();
        f.display("overloading");
    }
}

//ii) overriding -> different class same method
import java.util.*;
class Parents{
    void display(int a,int b){
        System.out.println(a+" "+b);
    }
}
class Children extends Parents{
    void display(int a){
         System.out.println(a);
    }
}
class Main{
    public static void main(String[]args){
        Children f=new Children();
        f.display(2);
    }
}

//Collatz Sequential:
import java.util.*;
public class Main{
    public static void main(String[]args){
        int n=5;
        int count =0;
		while(n!=1){
		    if(n%2==0){
		        n=n/2;
		    }
		    else{
		        n=3*n+1;
		    }
		    System.out.println(n+" ");
		    count++;
		}
		System.out.println(count);
    }
}
//Singleline array input and output
import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
    
        int a[]=new int[n];
        for(int i=0;i<n;a[i++]=sc.nextInt());
         for(int i=0;i<n;System.out.println(a[i++]+" "));
        
    }
}

//MultiThreading Concept
// i) Thread 
//maximum priority 10 //minimum priority 1//default 5 
// 0 1 2 3 4 5
class Mythread extends Thread{
public void run()//this place we can define method as run() or start()
{
    for(int i=0;i<3;i++){
System.out.println("Thread is running "+Thread.currentThread().getName());
System.out.println("Thread is running "+Thread.currentThread());
}
}
}
public class Main
{
public static void main(String[] args) {
Mythread t1 = new Mythread(); // New -> Runnable
t1.setName("Music"); //Set the name but its not priority based it will run 
Mythread t2 = new Mythread();
t2.setPriority(Thread.MAX_PRIORITY);
Mythread t3 = new Mythread();
Mythread t4 = new Mythread();                                                                 
Mythread t5 = new Mythread();
t1.start();// Running ->Wait->Terminated
t2.start();
t3.start();
t4.start();
t5.start();//t5.run(); if we run this the output is main because it only calling. 
}
}
// ii) Runnable interface
class Mythread implements Runnable{
public void run(){
    for(int i = 0 ; i < 10 ; i++){
        System.out.println("Thread is running "+Thread.currentThread().getName());
}
}
}
public class Main
{
public static void main(String[] args) {
Mythread t1 = new Mythread(); //
Thread r1 = new Thread(t1);
r1.start();
}
}
//index change the values

import java.util.ArrayList;
import java.util.Scanner;
public class Main{
    public static void main(String[]args){
      Scanner sc=new Scanner(System.in);
      int n=sc.nextInt();
      int arr[]=new int[n+1];
      for(int i=0;i<n;i++){
          arr[i]=sc.nextInt();
      }
    	System.out.println("Enter Index: ");
         int index = sc.nextInt()-1;
        System.out.println("Enter Value : ");
         int val = sc.nextInt();
        System.out.println("My array with lextra space ");
          for(int i = 0 ; i < n + 1 ; i++){
              System.out.print(arr[i]+" ");
          }
            for(int i= n; i>=index; i--){
                arr[i] = arr[i-1];
          }
       arr[index] = val;
       System.out.println();
       System.out.print("Final answer: ");
       for(int i = 0 ; i < n + 1 ; i++){
             System.out.print(arr[i]+" ");
          }

	}
}

//Collection functions:
import java.util.*;
public class Main{
   public static void main(String[]args){
       List<Integer> l=new ArrayList<>(Arrays.asList(1,2,3,4,5,3,4,2,1));
       
       Collections.sort(l);
       System.out.println(l);
       System.out.println("remove: "+l.remove(4));
       
        Collections.reverse(l);
       System.out.println("Reversed: "+l);
        Collections.shuffle(l);
       System.out.println("Shuffled: "+l);
       int f=Collections.frequency(l,4);
        System.out.println("Frequency: "+f);
        
   } 
}

//LinkedList:
import java.util.ArrayList;
import java.util.Collections;
import java.util.List;
import java.util.LinkedList;
public class Main{
   public static void main(String[]args){
       LinkedList<Integer> l=new LinkedList<>();
       l.add(0);
       l.add(1);
       l.add(3);
       l.add(0);
       l.add(1);
       l.add(3);
       System.out.println(l);
       System.out.println("remove first: "+l.removeFirst());
        System.out.println("remove last: "+l.removeLast());
        System.out.println("last: "+l.getLast());
         System.out.println("first: "+l.getFirst());
          System.out.println("Size: "+l.size());
        System.out.println("Contains: "+l.contains(3));
         System.out.println("Empty: "+l.isEmpty());
   } 
}

//Queue:
import java.util.Queue;
import java.util.Collections;
import java.util.List;
import java.util.LinkedList;
public class Main{
   public static void main(String[]args){
       Queue<Integer> l=new LinkedList<>(); //LinkedList or PriorityQueue 
       l.offer(0);
       l.offer(1);
       l.offer(3);
       l.offer(2);
       l.offer(1);
       l.offer(3);
       System.out.println(l);
       System.out.println("remove and return head: "+l.poll());
        System.out.println("view head without removing: "+l.peek());
        
        
   } 
}

// Map 
//It does not allow duplicate
//hash collision
import java.util.HashMap;
import java.util.Map;
public class Main {
public static void main(String[] args) {
	Map<Integer,String> n=new HashMap<>();
	//   key  value
	n.put(0, "apple");
	n.put(1, "orange");
	n.put(2, "kiwi");
	n.put(3, "strawberry");
	n.put(4, "berries");
	System.out.println(n);
	System.out.println(n.size());
	System.out.println(n.get(2));
	System.out.println(n.containsValue("blueberry"));
    System.out.println(n.entrySet());
	System.out.println(n.remove(4));
	System.out.println(n.values());
	System.out.println(n.keySet());
}
}

